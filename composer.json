{
	"name": "david63/schedulelock",
	"type": "phpbb-extension",
	"description": "An extension to schedule the locking of a topic.",
	"version": "3.3.0-RC3",
	"time": "2021-02-18",
	"keywords": [
		"phpbb",
		"extension",
		"topic",
		"lock"
	],
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "David Wood",
			"role": "Mod converter"
		}
	],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[d63] Schedule topic lock",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "schedule_topic_lock_version_file.json",
			"ssl": false
    	}
	}
